<h1>Movie list</h1>

<table class="records_list">
    <thead>
        <tr>
            <th>Id</th>
            <th>Name</th>
            <th>Synopsis</th>
            <th>Duration</th>
            <th>Releasedate</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
    {% for document in documents %}
        <tr>
            <td><a href="{{ path('movie_show', { 'id': document.id }) }}">{{ document.id }}</a></td>
            <td>{{ document.name }}</td>
            <td>{{ document.synopsis }}</td>
            <td>{{ document.duration }}</td>
            <td>{{ document.releasedate }}</td>
            <td>
                <ul>
                    <li>
                        <a href="{{ path('movie_show', { 'id': document.id }) }}">show</a>
                    </li>
                    <li>
                        <a href="{{ path('movie_edit', { 'id': document.id }) }}">edit</a>
                    </li>
                </ul>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>

<ul>
    <li>
        <a href="{{ path('movie_new') }}">
            Create a new entry
        </a>
    </li>
</ul>























